<h1>Log In</h1>
<% if flash[:notice]%>
<div class="notice"><%= flash[:notice] %></div>
<%end%>

<%= form_for :session, url: sessions_path do |f|%>
    <div class="field">
<%= f.label :email%>
<%= f.email_field :email %>
</div>

<div class="field">
<%= f.label :password %>
<%= f.password_field :password%>
</div>

<%= f.submit 'Login' %>
<% end %>

